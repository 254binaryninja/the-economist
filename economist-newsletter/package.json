{
  "name": "economist-newsletter",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "pnpm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.ts\"",
    "start": "pnpm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm:watch:ts\" \"pnpm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/sensible": "^6.0.0",
    "@getbrevo/brevo": "^2.5.0",
    "@supabase/supabase-js": "^2.50.3",
    "ai": "^4.3.16",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "inversify": "7.0.0-alpha.5",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "newsapi": "^2.4.1",
    "node-cron": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "rss-parser": "^3.13.0",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.1.0",
    "@types/node-cron": "^3.0.11",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~5.8.2"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
